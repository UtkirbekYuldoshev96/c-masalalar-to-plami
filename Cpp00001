#include <iostream>

using namespace std;

int haqiqiy(int a){
    if(a < 0){
        cout<<" - 1"<<endl;
    }else if(a == 0){
        cout<<" 0 nolga teng"<<endl;
    }else{
        cout<<" 1 "<<endl;
    }

}

int main()
{
    int x;
    cout<<" x = "; cin>>x;

    cout<<"natijalar = "<<haqiqiy(x)<<endl;

    return 0;
}
